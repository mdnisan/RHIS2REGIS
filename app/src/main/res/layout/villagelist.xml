<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"        android:layout_width="fill_parent"         android:layout_height="fill_parent"         android:overScrollMode="always"         android:isScrollContainer="true"         android:scrollbarAlwaysDrawVerticalTrack="true"         android:scrollbarStyle="outsideInset"         android:scrollbars="vertical">

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/popup"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical" >


    <LinearLayout android:layout_width="fill_parent" android:background="#F3F3F3" android:layout_height="wrap_content" android:orientation="horizontal" style="@style/rowStyle0">
        <TextView
            android:layout_width="90sp"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:text="উপজেলা"
            android:textColor="#000000"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/txtUpazila"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="10sp"
            android:textSize="20sp" />

        <TextView
            android:layout_width="90sp"
            android:layout_height="30sp"
            android:layout_marginLeft="20sp"
            android:gravity="left"
            android:text="ইউনিয়ন"
            android:textColor="#000000"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/txtUnion"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="10sp"
            android:textSize="20sp" />

    </LinearLayout>
    <View android:layout_height="1dip" android:layout_width="fill_parent" android:background="#CCCCCC" />
    <LinearLayout android:layout_width="fill_parent" android:background="#F3F3F3" android:layout_height="wrap_content" android:orientation="horizontal" style="@style/rowStyle0">
        <TextView android:id="@+id/lblAreaText"
            android:layout_width="90sp"
            android:layout_height="30sp"
            android:gravity="left"
            android:text="উপজেলাঃ"
            android:textColor="#000000"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/lblAreaName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10sp"
            android:textSize="20sp" />
    </LinearLayout>
    <LinearLayout android:visibility="gone" android:layout_width="fill_parent" android:background="#C2E0EC" android:layout_height="wrap_content" android:orientation="horizontal" style="@style/rowStyle0">
        <TextView
            android:layout_width="120sp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5sp"
            android:gravity="left"
            android:text="মৌজা"
            android:textColor="#000000"
            android:textSize="18sp" />

        <Spinner
            android:id="@+id/spnMouza" style="@style/customEdit"
            android:layout_width="380dp"
            android:layout_height="wrap_content"
            android:drawSelectorOnTop="true"
            android:prompt="@string/planet_prompt"
            android:textSize="16sp" />
    </LinearLayout>
    <View android:layout_height="1dip" android:layout_width="fill_parent" android:background="#CCCCCC" />



    <LinearLayout android:id="@+id/seclblH1" android:layout_width="fill_parent" android:layout_height="30sp" android:orientation="horizontal" android:background="#00B7FB" android:visibility="gone" >



        <TextView

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:text="  কর্ম এলাকা (গ্রামের তালিকা)"
            android:textColor="#FFFFFF"
            android:textSize="20sp" />

    </LinearLayout>

    <View android:layout_height="1dip" android:layout_width="fill_parent" android:background="#CCCCCC" />
    <LinearLayout android:visibility="gone" android:layout_width="fill_parent" android:background="#C2E0EC" android:layout_height="wrap_content" android:orientation="horizontal" style="@style/rowStyle0">
        <TextView
            android:layout_width="120sp"
            android:layout_height="30sp"
            android:layout_marginBottom="5sp"
            android:gravity="left"
            android:text="ওয়ার্ড(নতুন)"
            android:textColor="#000000"
            android:textSize="18sp" />

        <Spinner
            android:id="@+id/spnWardNew" style="@style/customEdit"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:drawSelectorOnTop="true"
            android:prompt="@string/planet_prompt"
            android:textSize="16sp" />

        <TextView
            android:layout_width="130sp"
            android:layout_height="30sp"
            android:layout_marginBottom="5sp"
            android:gravity="left"
            android:text="  ওয়ার্ড(পুরাতন)"
            android:textColor="#000000"
            android:textSize="18sp" />

        <Spinner
            android:id="@+id/spnWardOld" style="@style/customEdit"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="10sp"
            android:drawSelectorOnTop="true"
            android:prompt="@string/planet_prompt"
            android:textSize="16sp" />

    </LinearLayout>
    <View android:layout_height="1dip" android:layout_width="fill_parent" android:background="#CCCCCC" />

    <LinearLayout android:visibility="gone" android:id="@+id/secFWAUnit" android:background="#C2E0EC"  android:layout_width="fill_parent" android:layout_height="wrap_content" android:orientation="horizontal" style="@style/rowStyle0">
        <TextView android:id="@+id/VlblFWAUnit" android:text="ইউনিট" android:layout_width="120sp" android:layout_height="wrap_content" android:textSize="18sp" />
        <Spinner android:id="@+id/spnFWAUnit" android:drawSelectorOnTop="true" android:layout_width="150sp" android:layout_height="wrap_content" android:layout_weight="0" android:prompt="@string/planet_prompt" android:textSize="20sp" style="@style/customEdit"/>
        <TextView android:id="@+id/VlblEPIBlock" android:text="  ই পি আই ব্লক" android:layout_width="130sp" android:layout_height="wrap_content" android:textSize="18sp" />
        <Spinner android:id="@+id/spnEPIBlock" android:drawSelectorOnTop="true" android:layout_width="100sp" android:layout_height="wrap_content" android:layout_weight="0" android:prompt="@string/planet_prompt" android:textSize="20sp" style="@style/customEdit"/>
    </LinearLayout>

 <LinearLayout android:id="@+id/seclblH2" android:layout_width="fill_parent" android:layout_height="30sp" android:orientation="horizontal" android:background="#00B7FB">



        <TextView   android:id="@+id/VlblRegister"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:text="  পরিবার পরিকল্পনা পরিদর্শকের   তদারকি  কার্যক্রম"
            android:textColor="#FFFFFF"
            android:textSize="20sp" />

    </LinearLayout>



    <View android:layout_height="1dip" android:layout_width="fill_parent" android:layout_marginTop="0sp" android:layout_marginBottom="6sp" android:background="#D7D7D7"/>
    <LinearLayout android:id="@+id/linearLayout7" android:layout_height="wrap_content" android:gravity="left" android:layout_width="match_parent"  android:layout_marginBottom="10sp" android:visibility="visible">

        <Button android:text="পঃ কঃ সহকারীর মাসিক অগ্রিম কর্মসূচী অনুমোদন" android:id="@+id/cmdfpaworkplan"
            android:layout_width="170sp"
            android:layout_height="80sp" android:background="@drawable/button_style" android:layout_marginLeft="10sp" android:textSize="14sp"></Button>

        <Button android:text="পঃ কঃ সহকারীর মাসিক  কার্যক্রম তদারকি" android:id="@+id/cmdFPIworkplan1"
            android:layout_width="170sp"
            android:layout_height="80sp" android:background="@drawable/button_style" android:layout_marginLeft="10sp" android:textSize="14sp"></Button>

        <Button android:text="পঃ কঃ সহকারীর সংগৃহীত তথ্য যাচাই" android:id="@+id/cmdFPIworkplan2"
            android:layout_width="170sp"
            android:layout_height="80sp" android:background="@drawable/button_style" android:layout_marginLeft="10sp" android:textSize="14sp"></Button>

    </LinearLayout>
    <View android:layout_height="1dip" android:layout_width="fill_parent" android:layout_marginTop="0sp" android:layout_marginBottom="6sp" android:background="#D7D7D7"/>
    <LinearLayout  android:id="@+id/secfwaname" android:layout_width="fill_parent" android:background="#C2E0EC" android:layout_height="wrap_content" android:visibility="gone" android:orientation="horizontal" style="@style/rowStyle0">

        <TextView
            android:layout_width="200sp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5sp"
            android:gravity="left"
            android:text="পঃ কঃ সহকারীর নাম"
            android:textColor="#000000"
            android:textSize="18sp" />

        <Spinner
            android:id="@+id/spnProvider" style="@style/customEdit"
            android:layout_width="380dp"
            android:layout_height="wrap_content"
            android:drawSelectorOnTop="true"
            android:prompt="@string/planet_prompt"
            android:textSize="16sp" />
    </LinearLayout>

    <View android:id="@+id/viewa" android:layout_height="1dip" android:layout_width="fill_parent" android:layout_marginTop="0sp" android:layout_marginBottom="6sp" android:background="#D7D7D7" android:visibility="gone"/>
   <!-- তদারকি-->
    <LinearLayout android:id="@+id/linearLayoutElo" android:layout_height="wrap_content" android:gravity="left" android:layout_width="match_parent"  android:layout_marginBottom="10sp" android:visibility="gone">

        <Button android:text="জনসংখ্যা নিবন্ধন" android:id="@+id/cmdPrs"
            android:layout_width="170sp"
            android:layout_height="80sp" android:background="@drawable/button_style" android:layout_marginLeft="10sp" android:textSize="14sp"></Button>

        <Button android:text="দম্পতি " android:id="@+id/cmdElco"
            android:layout_width="170sp"
            android:layout_height="80sp" android:background="@drawable/button_style" android:layout_marginLeft="10sp" android:textSize="14sp"></Button>

        <Button android:text="গর্ভবতী মা ও নবজাতক " android:id="@+id/cmdmch"
            android:layout_width="170sp"
            android:layout_height="80sp" android:background="@drawable/button_style" android:layout_marginLeft="10sp" android:textSize="14sp"></Button>

    </LinearLayout>

    <View android:id="@+id/viewb" android:layout_height="1dip" android:layout_width="fill_parent" android:layout_marginTop="0sp" android:layout_marginBottom="6sp" android:background="#D7D7D7" android:visibility="gone"/>

    <LinearLayout  android:layout_width="fill_parent"  android:layout_height="200sp" android:visibility="gone"
        android:layout_weight="1"
        android:orientation="horizontal" android:scrollbars="vertical" style="@style/rowStyle0" android:layout_marginBottom="10sp" android:id="@+id/vList" >
        <GridView
            android:id="@+id/gridMenu"
            android:layout_width="wrap_content"
            android:layout_height="200sp"
            android:numColumns="4" android:layout_marginBottom="10sp" android:horizontalSpacing="5sp" />

    </LinearLayout>

    <LinearLayout android:id="@+id/seclblH3" android:layout_width="fill_parent" android:layout_height="30sp" android:orientation="horizontal" android:background="#00B7FB">



        <TextView   android:id="@+id/VlblRegister3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:text="  পরিবার পরিকল্পনা পরিদর্শকের  অন্যান্য কার্যক্রম"
            android:textColor="#FFFFFF"
            android:textSize="20sp" />

    </LinearLayout>

    <View android:layout_height="1dip" android:layout_width="fill_parent" android:layout_marginTop="0sp" android:layout_marginBottom="6sp" android:background="#D7D7D7"/>
    <LinearLayout android:id="@+id/linearLayout8" android:layout_height="wrap_content" android:gravity="left" android:layout_width="match_parent"  android:layout_marginBottom="10sp" android:visibility="visible">
        <Button android:text="পঃ পঃ পরিদর্শকের মাসিক অগ্রিম কর্মসূচী প্রনয়ন" android:id="@+id/cmdFPIworkplan"
            android:layout_width="170sp"
            android:layout_height="80sp" android:background="@drawable/button_style" android:layout_marginLeft="10sp" android:textSize="14sp"></Button>

        <Button android:text="পঃ পঃ পরিদর্শকের মাসিক অগ্রিম কর্মসূচী" android:id="@+id/cmdFPIworkplanview"
            android:layout_width="170sp"
            android:layout_height="80sp" android:background="@drawable/button_style" android:layout_marginLeft="10sp" android:textSize="14sp" android:visibility="gone"></Button>

        <Button android:text=" মাসিক অগ্রিম কর্মসূচী প্রনয়ন" android:id="@+id/cmdpaworkplan"
            android:layout_width="170sp"
            android:layout_height="80sp" android:background="@drawable/button_style" android:layout_marginLeft="10sp" android:textSize="14sp" android:visibility="gone"></Button>

    </LinearLayout>
    <View android:layout_height="1dip" android:layout_width="fill_parent" android:layout_marginTop="0sp" android:layout_marginBottom="6sp" android:background="#D7D7D7"/>


</LinearLayout>
</ScrollView>
