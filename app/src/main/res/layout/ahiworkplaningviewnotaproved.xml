                android:layout_marginBottom="1sp"
                android:layout_marginTop="1dip"
                android:background="@drawable/button_style"
                android:text="অনুমোদিত" android:visibility="gone"/>

            <TextView
                android:layout_width="20dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="" android:visibility="gone"/>

            <Button
                android:id="@+id/cmdNotApproved"
                android:layout_width="140dip"
                android:layout_height="80dip"
                android:layout_marginBottom="1sp"
                android:layout_marginTop="1dip"
                android:background="@drawable/button_style"
                android:text="অননুমোদিত" android:visibility="gone"/>

            <TextView
                android:layout_width="20dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="" />

            <Button
                android:id="@+id/cmdRequest"
                android:layout_width="140dip"
                android:layout_height="wrap_content"
                android:layout_marginBottom="1sp"
                android:layout_marginTop="1dip"
                android:background="@drawable/button_style"
                android:text="অপেক্ষাধিন" android:visibility="gone" />

        </LinearLayout>

        <LinearLayout style="@style/rowHeading"
