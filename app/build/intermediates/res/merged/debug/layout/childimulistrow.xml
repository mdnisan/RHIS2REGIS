<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="fill_parent"
    android:layout_height="fill_parent">
    <TableLayout
        android:id="@+id/memtab"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" android:layout_marginRight="10sp">

        <TableRow android:layout_marginTop="5sp" android:layout_marginBottom="5sp">
            <TextView android:id="@+id/imuCode"
                android:layout_width="1sp"
                android:gravity="center"
                android:layout_column="0" android:textSize="16sp"
                android:layout_height="wrap_content" android:visibility="invisible"/>
            <TextView android:id="@+id/imuName"
                android:layout_width="120sp"
                android:gravity="left" android:textSize="16sp"
                android:layout_column="1"
                android:layout_height="wrap_content"/>

            <TextView android:id="@+id/numOfDose"  android:textSize="16sp"
                android:layout_width="60sp"
                android:gravity="center"
                android:layout_column="2"
                android:layout_height="wrap_content"/>

            <TextView android:id="@+id/datetobegiven" android:textSize="16sp"
                android:layout_width="180sp"
                android:gravity="center"
                android:layout_column="3"
                android:layout_height="wrap_content"/>
            <TextView android:id="@+id/dategiven"  android:textSize="16sp"
                android:layout_width="180sp"
                android:gravity="center"
                android:layout_column="4"
                android:layout_height="wrap_content"/>
            <ImageButton
                android:id="@+id/btndate"
                android:layout_width="30sp"
                android:textSize="14sp"
                android:layout_marginTop="0sp"
                android:layout_height="30sp"
                android:text=""
                android:src="@drawable/calender"/>
        </TableRow>

    </TableLayout>
</RelativeLayout>
 
