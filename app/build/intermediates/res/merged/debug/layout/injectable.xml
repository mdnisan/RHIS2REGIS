            android:orientation="horizontal">

            <TextView
                android:id="@+id/VlblDOB"
                android:layout_width="150sp"
                android:layout_height="wrap_content"
                android:text="স্বামীর নাম"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/txtHusName"
                style="@style/customEdit"
                android:layout_width="220sp"
                android:layout_height="wrap_content"
                android:layout_marginRight="10sp"
                android:background="#D7D7D7"
                android:gravity="left"
                android:inputType="text"
                android:maxLength="100"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/VlblHusAge"
                android:layout_width="60sp"
                android:layout_height="wrap_content"
                android:text="বয়স "
                android:textSize="20sp" />
            <TextView
                android:id="@+id/txtHusAge"
                android:layout_width="50sp"
                android:layout_height="wrap_content"
                android:layout_marginRight="10sp"
                android:background="#D7D7D7"
                android:gravity="center"
                android:inputType="text"
                android:maxLength="100"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/VlblHusbosor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="(বছর)"
                android:textSize="20sp" />

        </LinearLayout>




        <LinearLayout
            android:id="@+id/secFatherName"
            style="@style/rowStyle0"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#F3F3F3"
