                android:background="#D7D7D7"
                android:gravity="left"
                android:inputType="text"
                android:maxLength="100"
                android:textSize="20sp" />



        </LinearLayout>
        <View
            android:layout_width="fill_parent"
            android:layout_height="1dip"
            android:background="#CCCCCC" />
        <LinearLayout
            android:id="@+id/secHouse1"
            style="@style/rowStyle0"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#F3F3F3"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/Vlbl1stDueDoze"
                android:layout_width="180sp"
                android:layout_height="wrap_content"
                android:text="প্রথম ডোজের তারিখ"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/dt1stDueDoze"
                style="@style/customEdit"
                android:layout_width="120sp"
                android:layout_height="wrap_content"
                android:layout_marginRight="10sp"
                android:background="#D7D7D7"
                android:gravity="left"
                android:inputType="text"
                android:maxLength="100"
                android:textSize="20sp" />


        </LinearLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="1dip"
            android:background="#CCCCCC" />

        <LinearLayout
            android:id="@+id/secDueDoze"
            style="@style/rowStyle0"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#F3F3F3"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/VlblDueDoze"
                android:layout_width="180sp"
                android:layout_height="wrap_content"
                android:text="ডিউ ডোজ"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/dtDueDoze"
                style="@style/customEdit"
                android:layout_width="120sp"
                android:layout_height="wrap_content"
                android:layout_marginRight="10sp"
                android:background="#D7D7D7"
                android:gravity="left"
                android:inputType="text"
                android:maxLength="100"
                android:textSize="20sp" />




        </LinearLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="1dip"
            android:background="#CCCCCC" />

        <LinearLayout
            android:id="@+id/secHHImplementdt"
            style="@style/rowStyle0"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#F3F3F3"
            android:orientation="horizontal" android:visibility="gone">


            <TextView
                android:id="@+id/VlblDozeDate"
                android:layout_width="180sp"
                android:layout_height="wrap_content"
                android:text="প্রয়োগের তারিখ "
                android:textSize="20sp" />

            <EditText
                android:id="@+id/dtDozeDate"
                style="@style/customEdit"
                android:layout_width="120sp"
                android:layout_height="wrap_content"
                android:layout_marginRight="10sp"
                android:focusable="false"
                android:inputType="text"
                android:textSize="20sp" />


            <ImageButton
                android:id="@+id/btnDozeDate"
                android:layout_width="40sp"
                android:layout_height="40sp"
                android:layout_gravity="center"
                android:src="@drawable/calender" />

            <ImageButton
                android:id="@+id/cmdSave"
                android:layout_width="40dp"
                android:layout_height="40sp"
                android:src="@drawable/save"
                android:text="Save" />

        </LinearLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="1dip"
            android:background="#CCCCCC" />

        <LinearLayout
            android:id="@+id/secPNCChH2"
