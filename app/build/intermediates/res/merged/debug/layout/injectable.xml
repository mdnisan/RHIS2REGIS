            android:layout_height="wrap_content"
            android:background="#F3F3F3"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/VlblSNo"
                android:layout_width="150sp"
                android:layout_height="wrap_content"
                android:text="ক্রমিক নং"
                android:textColor="#000000"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/txtSNo"
                android:layout_width="50sp"
                android:layout_height="wrap_content"
                android:layout_marginRight="10sp"
                android:background="#D7D7D7"
                android:gravity="center"
                android:inputType="text"
                android:maxLength="100"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/VlblELCONo"
                android:layout_width="92sp"
                android:layout_height="wrap_content"
                android:text="দম্পতি নং"
                android:textColor="#000000"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/ELCONo"
                android:layout_width="70sp"
                android:layout_height="wrap_content"
                android:layout_marginRight="10sp"
                android:background="#D7D7D7"
                android:gravity="center"
                android:inputType="text"
                android:maxLength="100"
                android:textSize="20sp" />


        </LinearLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="1dip"
            android:background="#CCCCCC" />

        <LinearLayout
            android:id="@+id/secName"
            style="@style/rowStyle0"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#F3F3F3"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/VlblName"
                android:layout_width="150sp"
                android:layout_height="wrap_content"
                android:text="গ্রহণকারীর নাম"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/txtName"
                style="@style/customEdit"
                android:layout_width="220sp"
                android:layout_height="wrap_content"
                android:layout_marginRight="10sp"
                android:background="#D7D7D7"
                android:gravity="left"
                android:inputType="text"
                android:maxLength="100"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/VlblAge"
                android:layout_width="60sp"
                android:layout_height="wrap_content"
                android:text="বয়স "
                android:textSize="20sp" />
            <TextView
                android:id="@+id/txtAge"
                android:layout_width="50sp"
                android:layout_height="wrap_content"
                android:layout_marginRight="10sp"
                android:background="#D7D7D7"
                android:gravity="center"
                android:inputType="text"
                android:maxLength="100"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/Vlblbosor"
